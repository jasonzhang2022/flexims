<?xml version="1.0" encoding="UTF-8"?>
<web-fragment
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://java.sun.com/xml/ns/javaee" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd" version="3.0">
 <name>report</name>
  <servlet>
    <servlet-name>com.flexdms.flexims.report.rs.RSApplication</servlet-name>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.flexdms.flexims.report.rs.RSApplication</servlet-name>
    <url-pattern>/reportrs/*</url-pattern>
  </servlet-mapping>
  <filter-mapping>
    <filter-name>EntityManagerFilter</filter-name>
    <servlet-name>com.flexdms.flexims.report.rs.RSApplication</servlet-name>
  </filter-mapping>
</web-fragment>